                                        <af:panelGroupLayout id="pgl2" layout="vertical" styleClass="WBCardSize">
                                            <af:panelBox text="PanelBox1" id="pb1" showDisclosure="false"
                                                         showHeader="never">
                                                <f:facet name="toolbar"/>
                                                <af:inputText value="#{bindings.BudgetSno.inputValue}"
                                                              label="#{bindings.BudgetSno.hints.label}"
                                                              required="#{bindings.BudgetSno.hints.mandatory}"
                                                              columns="#{bindings.BudgetSno.hints.displayWidth}"
                                                              maximumLength="#{bindings.BudgetSno.hints.precision}"
                                                              shortDesc="#{bindings.BudgetSno.hints.tooltip}" id="it5"
                                                              rendered="false">
                                                    <f:validator binding="#{bindings.BudgetSno.validator}"/>
                                                    <af:convertNumber groupingUsed="false"
                                                                      pattern="#{bindings.BudgetSno.format}"/>
                                                </af:inputText>
                                                <af:panelGridLayout id="pgl1">
                                                    <af:gridRow marginTop="5px" height="auto" id="gr2">
                                                        <af:gridCell marginStart="5px" width="100%" marginEnd="5px"
                                                                     id="gc2"
                                                                     styleClass="AFStretchWidth ERPHeroBackground"
                                                                     halign="stretch" valign="stretch">
                                                            <af:panelGroupLayout id="pgl6" layout="horizontal">
                                                                <af:link id="l1" styleClass="ERPHeroBannerFont ERPLink"
                                                                         icon="#{resource['images:imgView.png']}"
                                                                         action="DO-ACT-QUERY-FMS_0012_EDIT"
                                                                         shortDesc="Budget Short Name, Click To View Record."/>
                                                                <af:outputFormatted value="#{item.bindings.BudgetShortName.inputValue}"
                                                                                    id="of1"
                                                                                    styleClass="ERPHeroBannerFont"/>
                                                            </af:panelGroupLayout>
                                                        </af:gridCell>
                                                    </af:gridRow>
                                                    <af:gridRow marginTop="5px" height="auto" id="gr3">
                                                        <af:gridCell marginStart="5px" width="100%" marginEnd="5px"
                                                                     id="gc3">
                                                            <af:panelGroupLayout id="pgl7" layout="horizontal">
                                                                <af:image source="#{resource['images:imgDocumentNumber.png']}"
                                                                          id="i1" shortDesc="Budget Code"/>
