                                             selectedRowKeys="#{bindings.GlBudgetByCompanyRO.collectionModel.selectedRow}"
                                             selectionListener="#{bindings.GlBudgetByCompanyRO.treeModel.makeCurrent}"
                                             selection="single" styleClass="WBDisplayBlockCenter">
