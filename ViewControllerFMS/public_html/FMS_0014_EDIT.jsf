                                                                                      pattern="#{bindings.GlBankByCompanyRO.hints.BankShortCode.format}"/>
                                                                </af:outputFormatted>
                                                            </af:panelGroupLayout>
                                                        </af:gridCell>
                                                    </af:gridRow>
                                                    <af:gridRow marginTop="5px" height="auto" id="gr4">
                                                        <af:gridCell marginStart="5px" width="100%" marginEnd="5px"
                                                                     id="gc4">
                                                            <af:panelGroupLayout id="pgl8" layout="horizontal">
                                                                <af:image source="#{resource['images:imgShortName.png']}"
                                                                          id="i2" shortDesc="Budget Name"/>
                                                                <af:spacer width="10" height="10" id="s3"/>
                                                                <af:outputFormatted value="#{item.bindings.BudgetName.inputValue}"
                                                                                    id="of3"
                                                                                    styleClass="ERPHeroDetailColor"/>
                                                            </af:panelGroupLayout>
                                                        </af:gridCell>
                                                    </af:gridRow>
                                                    <af:gridRow marginTop="5px" height="auto" id="gr8">
                                                        <af:gridCell marginStart="5px" width="100%" marginEnd="5px"
                                                                     id="gc9">
                                                            <af:panelGroupLayout id="pgl9" layout="horizontal">
                                                                <af:image source="#{resource['images:imgStartDate.png']}"
                                                                          id="i3" shortDesc="Period Start Date"/>
                                                                <af:spacer width="10" height="10" id="s4"/>
                                                                <af:outputFormatted value="#{item.bindings.PeriodStartDate.inputValue}"
                                                                                    id="of4"
                                                                                    styleClass="ERPHeroDetailColor">
                                                                    <af:convertDateTime pattern="#{bindings.GlBankByCompanyRO.hints.OpeningDate.format}"/>
                                                                </af:outputFormatted>
                                                            </af:panelGroupLayout>
                                                        </af:gridCell>
                                                    </af:gridRow>
                                                    <af:gridRow marginTop="5px" height="auto" marginBottom="5px"
                                                                id="gr9">
                                                        <af:gridCell marginStart="5px" width="100%" marginEnd="5px"
                                                                     id="gc10">
                                                            <af:panelGroupLayout id="pgl10" layout="horizontal">
                                                                <af:image source="#{resource['images:imgStartDate.png']}"
                                                                          id="i4" shortDesc="Period End Date"/>
                                                                <af:spacer width="10" height="10" id="s5"/>
                                                                <af:outputFormatted value="#{item.bindings.PeriodEndDate.inputValue}"
                                                                                    id="of5"
                                                                                    styleClass="ERPHeroDetailColor"/>
                                                            </af:panelGroupLayout>
                                                        </af:gridCell>
                                                    </af:gridRow>
                                                </af:panelGridLayout>
